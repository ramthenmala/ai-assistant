{
  "name": "ai-chat-assistant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:database": "tsx src/scripts/test-database.ts",
    "test:database-mock": "tsx src/scripts/test-database-mock.ts",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && VITE_DEV_SERVER_URL=http://localhost:5173 electron .\"",
    "electron:build": "npm run build && vite build --config vite.electron.config.ts && electron-builder",
    "electron:preview": "npm run build && vite build --config vite.electron.config.ts && electron ."
  },
  "dependencies": {
    "@azure/msal-browser": "^4.15.0",
    "@azure/msal-react": "^3.0.15",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/pg": "^8.15.4",
    "@types/react-router-dom": "^5.3.3",
    "better-sqlite3": "^12.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dexie": "^4.0.11",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.292.0",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-resizable-panels": "^0.0.55",
    "react-router-dom": "^7.7.0",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@eslint/js": "^8.55.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^27.1.0",
    "electron-builder": "^24.6.4",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "globals": "^13.23.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.20.3",
    "typescript": "^5.2.2",
    "typescript-eslint": "^0.0.1-alpha.0",
    "vite": "^5.0.0",
    "vitest": "^0.34.6",
    "wait-on": "^7.2.0"
  }
